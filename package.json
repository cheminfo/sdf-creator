    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@types/jest": "^27.4.1",
    "cheminfo-build": "^1.1.11",
    "eslint": "^8.12.0",
    "eslint-config-cheminfo": "^7.3.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1"
