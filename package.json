{
  "name": "sdf-creator",
  "version": "2.0.3",
  "description": "Create SDF files",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root SDFCreator",
